<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="privatefileMapper">
	
	<insert id="insertNewPrivateFile" parameterType="int">
		insert into privatefile values(SEQ_PFILENO.NEXTVAL, #{userno}, null, null, default, sysdate, default)
	</insert>
	
	<select id="selectList" parameterType="int" resultType="privateFile">
		select * from privatefile where userno = #{userno}
	</select>
		
</mapper>